                              <Insets right="3.0" />
                           </HBox.margin>
                           <children>
                              <Button fx:id="addFileButton" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#addFiles" prefHeight="25.0" prefWidth="25.0">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="2.0" top="25.0" />
                                 </VBox.margin>
                              </Button>
                              <Button fx:id="removeFileButton" layoutX="12.0" layoutY="12.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#removeFile" prefHeight="25.0" prefWidth="25.0">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="2.0" top="2.0" />
                                 </VBox.margin>
                              </Button>
                              <Button fx:id="clearFileButton" layoutX="12.0" layoutY="39.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#clearFiles" prefHeight="25.0" prefWidth="25.0">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="2.0" top="2.0" />
                                 </VBox.margin>
                              </Button>
                              <Button fx:id="testRunButton" layoutX="10.0" layoutY="93.0" minHeight="25.0" minWidth="25.0" mnemonicParsing="false" onAction="#testRun" prefHeight="25.0" prefWidth="25.0" text="F">
                                 <font>
                                    <Font name="System Bold" size="12.0" />
                                 </font>
                              </Button>
                           </children>
                        </VBox>
                        <VBox prefHeight="303.0" prefWidth="838.0">
                           <children>
                              <HBox alignment="TOP_RIGHT" prefHeight="17.0" prefWidth="578.0">
                                 <children>
                                    <Region layoutX="424.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0" />
                                    <Label alignment="CENTER" minWidth="68.0" prefHeight="17.0" prefWidth="68.0" text="Input" textAlignment="CENTER" HBox.hgrow="NEVER">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Label>
                                    <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                                    <Region layoutX="228.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0" />
                                    <Label alignment="CENTER" minWidth="68.0" prefHeight="17.0" prefWidth="68.0" text="Output" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
                                    <Region layoutX="56.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0" />
                                 </children>
                              </HBox>
                              <ListView fx:id="inputFileList" minWidth="550.0" onDragDropped="#handleFileDrop" onDragOver="#handleFileDragOver" prefHeight="303.0" prefWidth="600.0">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                              </ListView>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <TextField fx:id="logBox" editable="false" text="Default text" />
            <ProgressBar maxHeight="25.0" minHeight="25.0" prefHeight="25.0" prefWidth="520.0" progress="0.79" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="3.0" top="3.0" />
               </VBox.margin></ProgressBar>
            <TextArea editable="false" prefHeight="79.0" prefWidth="490.0" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
